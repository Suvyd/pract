#include <stdio.h>

// Функція для підрахунку кількості шуканих послідовностей
int countSequences(int n) {
    int count0 = 1;  // Кількість допустимих послідовностей довжиною 1, закінчуючи нулем
    int count1 = 1;  // Кількість допустимих послідовностей довжиною 1, закінчуючи одиницею

    for (int i = 2; i <= n; i++) {
        // Оновлення кількостей допустимих послідовностей
        int newCount0 = (count0 + count1) % 12345;  // Кількість нових послідовностей, закінчуючи нулем
        int newCount1 = count0;  // Кількість нових послідовностей, закінчуючи одиницею

        // Оновлення значень кількостей допустимих послідовностей
        count0 = newCount0;
        count1 = newCount1;
    }

    // Повертаємо кількість шуканих послідовностей за модулем 12345
    return (count0 + count1) % 12345;
}

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    printf("Bohdan Suvyd\n");
    int n;
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    // Виклик функції countSequences і вивід результату
    int result = countSequences(n);
    printf("Кількість шуканих послідовностей за модулем 12345: %d\n", result);

    return 0;
}
